{
  "name": "szero",
  "version": "1.0.0",
  "description": "Sub Zero dependency search.",
  "main": "index.js",
  "scripts": {
    "test": "nyc tape test/*.js | tap-spec",
    "coverage": "nyc report --reporter=text-lcov > ./coverage/lcov.info",
    "lint": "eslint test/*.js index.js lib/*.js",
    "docs": "./node_modules/.bin/jsdoc --verbose -d docs -t ./node_modules/ink-docstrap/template -R README.md index.js ./lib/*.js",
    "prepublish": "nsp check",
    "release": "standard-version",
    "licenses": "license-reporter report"
  },
  "files": [
    "bin",
    "LICENSE.txt",
    "package.json",
    "README.md",
    "lib"
  ],
  "author": "Helio Frota",
  "repository": {
    "type": "git",
    "url": "bucharest-gold/szero"
  },
  "dependencies": {
    "commander": "^2.9.0",
    "decomment": "^0.9.1",
    "node-builtins": "^0.1.1",
    "roi": "^0.16.1"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "eslint": "^4.13.1",
    "eslint-config-semistandard": "^11.0.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-standard": "^3.0.1",
    "ink-docstrap": "^1.3.0",
    "jsdoc": "^3.4.2",
    "license-reporter": "^1.0.2",
    "nsp": "^3.1.0",
    "nyc": "^11.3.0",
    "standard-version": "^4.2.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0",
    "test-console": "^1.0.0"
  },
  "preferGlobal": true,
  "bin": {
    "szero": "./bin/szero.js"
  }
}
